$bog_theme_picker $mol_pop
	Anchor $mol_view
	theme_auto $bog_theme_auto
	query?val \
	focused_index?val -1
	body /
		<= Search $mol_string
			value?val <=> query?val
			hint @ \Search themes
		<= Theme_list $mol_list
			rows <= theme_rows /
	event *
		^
		keydown?event <=> key_down?event null
	theme_row* $bog_theme_picker_row
		theme_name <= theme_name* \
		clicked?event <=> theme_clicked*?event null
		hovered?event <=> theme_hovered*?event null
